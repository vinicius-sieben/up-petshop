package front;

public class App {

	public static void main(String[] args) {
		do {
			char op;
			System.out.println("-------- PETSHOP --------");
			System.out.println("[1] - Cadastrar cliente");
			
			switch(op) {
			case 1:
				System.out.println("Funcionando");
				break;
			default:
				System.out.println("Opção inválida");
			}
			
		} while (op != 6);

	}

}
