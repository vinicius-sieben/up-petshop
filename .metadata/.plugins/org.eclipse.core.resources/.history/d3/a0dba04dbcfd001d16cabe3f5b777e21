package front;

public class App {

	public static void main(String[] args) {

		int op;
		
		do {
			System.out.println("-------- PETSHOP --------");
			System.out.println("[1] - Cadastrar cliente");
			op = Console.readInt("Selecione uma opcao: ");
			
			switch(op) {
			
			case 1:
				System.out.println("Funcionou");
				break;
			default:
				System.out.println("Opcao invalida!");
				
			}
			
			
		} while (op != 1);
		
	}

}
